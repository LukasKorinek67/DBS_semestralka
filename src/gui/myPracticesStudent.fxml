<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" styleClass="mainFxmlClass" stylesheets="@student.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MyPracticesStudentController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="50.0" top="30.0" />
         </padding>
         <children>
            <HBox prefHeight="20.0" prefWidth="200.0">
               <children>
                  <Label text="Přihlášený student: " />
                  <Label fx:id="loggedStudent" text="Student Student" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </HBox>
            <Label text="Moje praxe">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <GridPane alignment="CENTER_LEFT" hgap="10.0" vgap="20.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="studiedProgrammes" text="Studovaný obor:" />
                  <Label text="Přiřazená praxe:" GridPane.columnIndex="1" />
                  <Label fx:id="studyProgramme1" text="Studijní obor 1" GridPane.rowIndex="1" />
                  <Label fx:id="studyProgramme2" text="Studijní obor 2" GridPane.rowIndex="2" />
                  <Label fx:id="studyProgramme3" text="Studijní obor 3" GridPane.rowIndex="3" />
                  <Label fx:id="studyProgramme4" text="Studijní obor 4" GridPane.rowIndex="4" />
                  <Label fx:id="studyProgramme5" text="Studijní obor 5" GridPane.rowIndex="5" />
                  <Label fx:id="practice1" text="Praxe 1" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="practice2" text="Praxe 2" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="practice3" text="Praxe 3" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="practice4" text="Praxe 4" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="practice5" text="Praxe 5" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
                     <children>
                        <Button fx:id="buttonInfoPractice1" minWidth="120.0" mnemonicParsing="false" onAction="#displayPractice1" text="Info o praxi" />
                        <Button fx:id="buttonSubmitMessagePractice1" mnemonicParsing="false" onAction="#submitMessagePractice1" text="Odevzdat zprávu z praxe" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="2">
                     <children>
                        <Button fx:id="buttonInfoPractice2" minWidth="120.0" mnemonicParsing="false" onAction="#displayPractice2" text="Info o praxi" />
                        <Button fx:id="buttonSubmitMessagePractice2" mnemonicParsing="false" onAction="#submitMessagePractice2" text="Odevzdat zprávu z praxe" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="3">
                     <children>
                        <Button fx:id="buttonInfoPractice3" minWidth="120.0" mnemonicParsing="false" onAction="#displayPractice3" text="Info o praxi" />
                        <Button fx:id="buttonSubmitMessagePractice3" mnemonicParsing="false" onAction="#submitMessagePractice3" text="Odevzdat zprávu z praxe" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="4">
                     <children>
                        <Button fx:id="buttonInfoPractice4" minWidth="120.0" mnemonicParsing="false" onAction="#displayPractice4" text="Info o praxi" />
                        <Button fx:id="buttonSubmitMessagePractice4" mnemonicParsing="false" onAction="#submitMessagePractice4" text="Odevzdat zprávu z praxe" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="5">
                     <children>
                        <Button fx:id="buttonInfoPractice5" minWidth="120.0" mnemonicParsing="false" onAction="#displayPractice5" text="Info o praxi" />
                        <Button fx:id="buttonSubmitMessagePractice5" mnemonicParsing="false" onAction="#submitMessagePractice5" text="Odevzdat zprávu z praxe" />
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets left="30.0" right="30.0" />
               </VBox.margin>
            </GridPane>
            <Button fx:id="buttonDisplayApplications" mnemonicParsing="false" onAction="#displayMyApplications" text="Zobrazit moje žádosti o praxe" />
            <Label fx:id="errorMessage" text="ErrorMessage" />
            <Hyperlink fx:id="backLink" onAction="#goBack" text="Zpět" />
         </children>
      </VBox>
      <HBox alignment="BOTTOM_RIGHT" layoutX="20.0" layoutY="20.0" prefHeight="40.0" prefWidth="350.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Label text="DBS - Semestrální práce - Böhm, Kořínek, Rubeš" />
         </children>
         <padding>
            <Insets bottom="10.0" right="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
